<Application x:Class="ClientWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ClientWPF"
			 Exit="Application_Exit" 
			 Startup="Application_Startup">
    <Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="UIKit/ColorStyles.xaml"/>
				<ResourceDictionary Source="UIKit/ButtonStyles.xaml"/>
				<ResourceDictionary Source="UIKit/FontStyles.xaml"/>
				<ResourceDictionary Source="UIKit/TextBlockStyles.xaml"/>
				<ResourceDictionary Source="UIKit/TextBoxStyles.xaml"/>
				<ResourceDictionary Source="UIKit/SliderStyles.xaml"/>
				<ResourceDictionary Source="UIKit/ScrollViewerStyles.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
